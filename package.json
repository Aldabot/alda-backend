{
  "dependencies": {
    "alda-dialogflow": "^0.5.0",
    "alda-graphql-server": "../alda-graphql-server",
    "aws-serverless-express": "^3.3.5",
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "graphql-request": "^1.8.2"
  },
  "scripts": {
    "buildDocker": "docker build -t alda/backend-deploy .",
    "deploy": "export $(xargs <.env) && rm -rf node_modules && docker run --rm -e AWS_ACCESS_KEY_ID -e AWS_SECRET_ACCESS_KEY -e WEB_VIEW_URL alda/backend-deploy",
    "start": "export $(xargs <.env) && sls offline",
    "fbLogs": "sls log -f fbWebhook -t"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
